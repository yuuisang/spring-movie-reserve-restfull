<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goldspoon.koreabox.screeninfo.beans.ScreenInfoDAO">

	<!-- 
		매퍼파일에서 auto-generated key 값 받아오기 설정
	 -->
	 <insert id="insert" flushCache="true" 
	 		parameterType="com.goldspoon.koreabox.screeninfo.beans.ScreenInfoDTO" useGeneratedKeys="true"
	 		keyColumn="wr_uid" 
	 		keyProperty="uid"
	 		>
	 		INSERT INTO test_write
				(wr_uid, wr_subject, wr_content, wr_name, wr_regdate)
			VALUES
				(test_write_seq.nextval, #{subject}, #{content}, #{name}, SYSDATE)
	 </insert>


